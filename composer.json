{
    "name": "blashbrook/papi",
    "description": "Custom self-registration form for Polaris ILS",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Lashbrook",
            "email": "blashbrook@gmail.com",
            "homepage": "https://github.com/blashbrook"
        }
    ],
    "homepage": "https://github.com/blashbrook/papi",
    "keywords": ["Laravel", "Papi"],
    "require": {
        "illuminate/support": "~7|~8",
        "orchestra/testbench": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Blashbrook\\Papi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Blashbrook\\Papi\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blashbrook\\Papi\\PapiServiceProvider"
            ],
            "aliases": {
                "Papi": "Blashbrook\\Papi\\Facades\\Papi"
            }
        }
    }
}
